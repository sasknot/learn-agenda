{
  "name": "learn-agenda",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "lint": "stylelint **/*.{css,scss,vue} && vue-tsc --noEmit && eslint --ext .js,.ts,.tsx,.vue src",
