{
  "name": "learn-agenda",
  "version": "0.0.0",
  "scripts": {
    "test": "vitest run src",
    "test:watch": "vitest watch src",
    "test:coverage": "vitest run --coverage",
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "lint": "vue-tsc --noEmit && stylelint **/*.{css,scss,vue} && eslint src --ext .js,.ts,.jsx,.tsx,.vue",
    "lint:fix": "vue-tsc --noEmit && stylelint --fix **/*.{css,scss,vue} && eslint --fix src --ext .js,.ts,.jsx,.tsx,.vue --fix"
  },
  "dependencies": {
    "@tailwindcss/forms": "^0.5.7",
    "@vee-validate/zod": "^4.12.6",
    "vee-validate": "^4.12.6",
    "vue": "^3.4.21",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@nabla/vite-plugin-eslint": "^2.0.2",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitest/coverage-v8": "^1.4.0",
    "@vue/test-utils": "^2.4.5",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-plugin-filenames-simple": "^0.9.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^17.2.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-tailwindcss": "^3.15.1",
    "eslint-plugin-vue": "^9.24.1",
    "jsdom": "^24.0.0",
    "stylelint": "^16.3.1",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recommended-scss": "^14.0.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "tailwindcss": "^3.2.4",
    "typescript": "^5.4.5",
    "vite": "^5.2.8",
    "vite-plugin-stylelint": "^5.3.1",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.4.0",
    "vue-eslint-parser": "^9.4.2",
    "vue-tsc": "^2.0.12"
  }
}
