{
  "name": "learn-agenda",
  "version": "0.0.0",
  "scripts": {
    "test": "vitest run src",
    "test:watch": "vitest watch src",
    "test:coverage": "vitest run --coverage",
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "lint": "vue-tsc --noEmit && stylelint **/*.{css,scss,vue} && eslint src --ext .js,.ts,.jsx,.tsx,.vue",
    "lint:fix": "vue-tsc --noEmit && stylelint --fix **/*.{css,scss,vue} && eslint --fix src --ext .js,.ts,.jsx,.tsx,.vue --fix"
  },
  "dependencies": {
    "@tailwindcss/forms": "^0.5.3",
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@babel/types": "^7.21.4",
    "@nabla/vite-plugin-eslint": "^1.5.0",
    "@types/node": "^18.15.12",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vitest/coverage-c8": "^0.30.1",
    "@vue/test-utils": "^2.3.2",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.38.0",
    "eslint-plugin-filenames-simple": "^0.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-tailwindcss": "^3.11.0",
    "eslint-plugin-vue": "^9.11.0",
    "jsdom": "^21.1.1",
    "postcss": "^8.4.19",
    "postcss-html": "^1.5.0",
    "stylelint": "^14.16.1",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recommended-scss": "^8.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.9.4",
    "vite": "^4.3.0",
    "vite-plugin-stylelint": "^4.3.0",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^0.30.1",
    "vue-eslint-parser": "^9.1.1",
    "vue-tsc": "^1.2.0"
  }
}
